{
  "name": "eslint-plugin-overdrive",
  "version": "0.1.0",
  "description": "A collection of ESLint rules",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@9.4.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/eslint-plugin-overdrive.git"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "plugin",
    "linting",
    "eslint-plugin",
    "rules",
    "eslint-rules"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "dev": "tsup --watch",
    "lint": "eslint . --report-unused-disable-directives",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": ">=8.40.0"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.5.0",
    "@luxass/eslint-utils": "^0.1.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/rule-tester": "^8.0.0",
    "@typescript-eslint/utils": "^8.0.0",
    "eslint": "^9.4.0",
    "eslint-vitest-rule-tester": "^1.0.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2",
    "vitest": "^3.0.0"
  }
}
